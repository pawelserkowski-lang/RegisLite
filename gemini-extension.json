{
  "id": "jules-regislite",
  "name": "Jules (RegisLite)",
  "version": "5.0.0",
  "publisher": "gemini-cli-extensions",
  "description": "System autonomicznej naprawy kodu z interfejsem WebSocket i Agentem AI (Jules Extension Auditor v4.0).",
  "icon": "icon.png",
  "main": "run.py",
  "engines": {
    "vscode": "^1.80.0",
    "gemini": "^1.0.0"
  },
  "activationEvents": [
    "onCommand:jules.start",
    "onCommand:jules.audit"
  ],
  "contributes": {
    "commands": [
      {
        "command": "jules.start",
        "title": "Jules: Start Agent"
      },
      {
        "command": "jules.audit",
        "title": "Jules: Run Audit (SoT)"
      }
    ],
    "configuration": {
      "title": "Jules AI",
      "properties": {
        "jules.mode": {
          "type": "string",
          "default": "autonomous",
          "enum": [
            "autonomous",
            "interactive"
          ],
          "description": "Operational mode for the Jules agent."
        }
      }
    }
  },
  "gemini": {
    "systemPrompt": "GEMINI.md",
    "capabilities": [
      "code-repair",
      "multi-agent-debate",
      "skeleton-of-thought"
    ]
  }
}
